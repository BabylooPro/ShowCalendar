{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"IpRateLimiting": {
		"EnableEndpointRateLimiting": true,
		"StackBlockedRequests": false,
		"RealIpHeader": "X-Real-IP",
		"ClientIdHeader": "X-ClientId",
		"HttpStatusCode": 429,
		"GeneralRules": [
			{
				"Endpoint": "*",
				"Period": "1s",
				"Limit": 10
			},
			{
				"Endpoint": "*",
				"Period": "1m",
				"Limit": 1000
			},
			{
				"Endpoint": "*",
				"Period": "1h",
				"Limit": 10000
			},
			{
				"Endpoint": "*",
				"Period": "1d",
				"Limit": 100000
			}
		]
	},
	"AllowedHosts": "*"
}
